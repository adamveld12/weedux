{
  "name": "weedux",
  "version": "3.0.1",
  "description": "A toy redux like thing",
  "main": "dist/index.js",
  "jsnext:main": "src/index.js",
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "test": "jest -b",
    "dev": "jest --colors --watch -o --runInBand ",
    "build": "babel src --out-dir dist",
    "prepublish": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/adamveld12/weedux.git"
  },
  "keywords": [
    "react",
    "flux"
  ],
  "homepage": "https://github.com/adamveld12/weedux#readme",
  "author": "Adam Veldhousen",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/adamveld12/weedux/issues"
  },
  "babel": {
    "presets": [
      "es2015"
    ]
  },
  "jest": {
    "scriptPreprocessor": "<rootDir>/node_modules/babel-jest",
    "testPathDirs": [
      "./src"
    ]
  },
  "devDependencies": {
    "babel-cli": "^6.18.0",
    "babel-jest": "^16.0.0",
    "babel-polyfill": "^6.16.0",
    "babel-preset-es2015": "^6.18.0",
    "jest": "^16.0.2"
  },
  "dependencies": {
    "immutable": "^3.8.1"
  }
}
